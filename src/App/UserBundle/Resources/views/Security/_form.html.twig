<form action="{{ path("fos_user_security_check") }}" method="post" name="LoginForms">
        <div>            
            <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />         
        </div>    
        <div>
            <label for="username">{{ 'security.login.username'|trans({}, 'FOSUserBundle') }}</label>            
        </div>
        <div>
            <input type="text" id="username" name="_username" value="{{ last_username }}" placeholder="{{ 'security.login.username'|trans({}, 'FOSUserBundle') }}" />
        </div>
        <div>
            <label for="password">{{ 'security.login.password'|trans({}, 'FOSUserBundle') }}</label>
        </div>
        <div>
            <input type="password" id="password" name="_password" placeholder="{{ 'security.login.password'|trans({}, 'FOSUserBundle') }}"/>
        </div>

        <div>
           <input type="checkbox" id="remember_me" name="_remember_me" value="on" />
            <label for="remember_me">{{ 'security.login.remember_me'|trans({}, 'FOSUserBundle') }}</label>
        </div>
        <div>
            <input type="submit" class="submit-login" name="LoginFormBtn" value="{{ 'security.login.submit'|trans({}, 'FOSUserBundle') }}" />
        </div>
        <div class="clearfix"></div>
        <br />
        <div class="separator">
            <p class="change_link">
                <a href="#forgotbox" class="to_register forgot-pwd">{{ 'Zapomniałeś hasła?'|trans({}, 'gsSecurity') }}</a>
                {#<a href="#signup" class="to_register"> Create Account </a>#}
            </p>

            <div class="clearfix"></div>
        </div>
</form>


